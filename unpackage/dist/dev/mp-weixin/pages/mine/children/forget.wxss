.content {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.tishi {
	color: #FFFFFF;
	font-size: 28rpx;
	line-height: 50rpx;
	margin-bottom: 50rpx;
	margin-left: 50rpx;
}
.list {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	padding-top: 0rpx;
	padding-left: 0rpx;
	padding-right: 0rpx;
	margin: 0 50rpx;
	background-color: #FFFFFF;
	border-radius: 10rpx;
}
.list-call{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	height: 100rpx;
	color: #333333;
	border-bottom: 1rpx solid rgba(230,230,230,1);
}
.list-call .img{
	width: 40rpx;
	height: 40rpx;
	margin-left: 20rpx;
}
.list-call .biaoti{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	text-align: left;
	font-size: 30rpx;
	line-height: 100rpx;
	margin-left: 16rpx;
}
.dlbutton {
	color: #FFFFFF;
	font-size: 34rpx;
	width:85%;
	height:100rpx;
	background:-webkit-linear-gradient(135deg,#4adfff,#6fffff);
	background:linear-gradient(-45deg,#4adfff,#6fffff);
	/* box-shadow:0upx 0upx 13upx 0upx rgba(164,217,228,0.2); */
	border-radius:50rpx;
	line-height: 100rpx;
	text-align: center;
	margin-left: auto;
	margin-right: auto;
	margin-top: 100rpx;
}
.dlbutton-hover {
	background:-webkit-linear-gradient(#4adfff);
	background:linear-gradient(#4adfff);
}
.dlbutton-none {
	color: #FFFFFF;
	font-size: 34rpx;
	width:85%;
	height:100rpx;
	background:#323241;
	/* box-shadow:0upx 0upx 13upx 0upx #6fffff; */
	border-radius:50rpx;
	line-height: 100rpx;
	text-align: center;
	margin-left: auto;
	margin-right: auto;
	margin-top: 100rpx;
	color: #50505c;
}
.yzm {
	color: #333333;
	font-size: 24rpx;
	line-height: 64rpx;
	padding-left: 10rpx;
	padding-right: 10rpx;
	width:auto;
	height:64rpx;
	border:1rpx solid #333333;
	border-radius: 50rpx;
	margin-right: 10rpx;
}
.yzms {
	color: #999999 !important;
	border:1rpx solid #999999;
}

